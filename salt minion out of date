#!py

import salt.client
import json

local = salt.client.LocalClient()

dump = json.dumps(local.cmd('*','test.version'))
data = json.loads(dump)

text_file = open("Minion's needing updates", "w")

vn = raw_input("Please enter VN:  ")
for k, v in data.items():
    if v != vn:
        text_file.write(k + "\n")